<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
	  <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
	  	<td><%= course.code %></td>
        <td><%= course.name %></td>
		<td><%= course.desc %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>