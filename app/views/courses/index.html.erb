<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Subjects</th>
      <th>Requirements</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.name %></td>
        <td><%= course.subjects %></td>
        <td><%= course.requirements %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>